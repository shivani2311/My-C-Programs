#include<stdio.h>
#include<conio.h>

int main()
{
			 int No = 0, Min = 0, Cnt = 0;
			 
			 printf("\n Enter 10 Number =>");
			 
			 for(Cnt = 1; Cnt <=10; Cnt++)
			 {
			 			printf("\n\t Enter Number %d=",Cnt);
			 			scanf("%d",&No);
			 			
			 			if(Cnt==1)
			 			{
			 					Min = No;
			 					continue;
			 			}
			 			
			 			if(No < Min)
			 			{
			 					 Min = No;
			 			}
			 }
			 
			 printf("\n Press Any Key To Get Minimum From Given Number =>");
			 getche();
			 
			 printf("\n Minimum of Given Number =%d",Min);
			 
			 printf("\n\n Thanks");
			 
			 getch();
			 return 0;
}