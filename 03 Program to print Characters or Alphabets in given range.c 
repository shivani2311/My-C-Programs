#include<stdio.h>
#include<conio.h>

int main()
{
	char L_Char='\0',S_Char='\0',E_Char='\0';
	
	printf("\n Enter Starting Character=");
	S_Char=getche();
	
	printf("\n Enter Ending Character=");
	E_Char=getche();
	
	for(L_Char=S_Char; L_Char<=E_Char; L_Char++)
	{
		printf("\n %c",L_Char);
	}
	
	printf("\n\n Thanks!!!");
	
	getch();
	return 0;
}